{
  "name": "techbook-ranking-backend",
  "version": "1.0.0",
  "description": "Backend API for TechBook Ranking service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "fetch-qiita": "ts-node -r tsconfig-paths/register scripts/fetchQiita.ts",
    "enrich-books": "ts-node -r tsconfig-paths/register scripts/enrichBooks.ts",
    "cron-runner": "ts-node -r tsconfig-paths/register scripts/cronRunner.ts",
    "cron-daemon": "ts-node -r tsconfig-paths/register scripts/cronRunner.ts daemon",
    "cron-app": "ts-node -r tsconfig-paths/register src/cronApp.ts",
    "test-slack": "ts-node -r tsconfig-paths/register scripts/testSlack.ts",
    "seed": "ts-node -r tsconfig-paths/register scripts/seedDatabase.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "crypto": "^1.0.1",
    "url": "^0.11.3",
    "node-cron": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "bcrypt": "^5.1.1",
    "@slack/web-api": "^7.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/node-cron": "^3.0.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/bcrypt": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "express",
    "typescript",
    "mongodb",
    "mongoose",
    "api",
    "books",
    "ranking"
  ],
  "author": "",
  "license": "MIT"
}